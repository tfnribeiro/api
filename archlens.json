{
  "$schema": "https://raw.githubusercontent.com/archlens/ArchLens/master/src/config.schema.json",
  "name": "zeeguu-api",
  "rootFolder": "zeeguu",
  "github": {
    "url": "https://github.com/zeeguu/api",
    "branch": "master"
  },
  "saveLocation": "./diagrams/",
  "views": {
    "top-level-view-depth-1": {
      "packages": [
        {
          "path": "*",
          "depth": 1
        }
      ],
      "ignorePackages": []
    },
    "inside-api": {
      "packages": [
        {
          "path": "api",
          "depth": 1
        }
      ],
      "ignorePackages": [
        "*test*",
        "core"
      ]
    },
    "inside-core": {
      "packages": [
        {
          "path": "core",
          "depth": 1
        }
      ],
      "ignorePackages": [
        "*test*",
        "core"
      ]
    }
  }
}
